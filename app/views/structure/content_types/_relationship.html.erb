<div class="relationship">
 	<%= f.fields_for :relationships, relationship, :index => nil do |f_nested| %>
    	<%= f_nested.select(:type, Structure::Relationship.get_type_options.collect {|f| [f, Structure::Relationship.get_type_options.index(f)]}) %>
    	<%= f_nested.select(:to_id, Structure::ContentType.all.collect {|c| [c.name, c._id]}) %>
    	<!-- TODO: Fix embedded JS -->
    	<%= link_to_function "(remove)", "$(this).parent().remove()" %>
	<% end %>
</div>